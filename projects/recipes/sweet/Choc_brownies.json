{
    "ingredients":
        [
            {
                "section": "",
                "items": [
                    "400g can black beans",
                    "2 large eggs",
                    "2 bananas (200g)",
                    "3 Tbsp maple syrup",
                    "15g cocoa powder",
                    "60g protein powder (2 scoops)",
                    "1 tsp baking powder",
                    "pinch of salt"
                ]
            }
        ],
    "instructions":
        [
            {
                "section": "",
                "steps": [
                        "Preheat the oven to 180°C.",
                        "Line a 25cm x 15cm tin with parchment paper.",
                        "Add beans and banana to a bowl and blitz with stick blender.",
                        "Add all ingredients into a bowl and mix well with a fork or spoon.",
                        "Pour ingredients into lined tin and place in preheated oven.",
                        "Bake at 180° for 15 minutes.",
                        "Rest for 15 minutes to cool in tin, then place on wire rack (parchment underneath) and cool for at least 45 minutes."
                    ]
            }
        ],
